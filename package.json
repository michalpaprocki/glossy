{
  "name": "eve_glossary",
  "version": "0.0.0",
  "description": "simple rest api providing eve online related glossary ",
  "main": "app.js",
  "scripts": {
    "test": "vitest",
    "dev": "nodemon app.ts",
    "prettier": "npx prettier . --write",
    "build": "tsc",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "db:generate": "drizzle-kit generate --dialect=postgresql --schema=src/db/schema/* --out=./drizzle",
    "db:migrate": "tsx ./src/db/migrate.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.23.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.4",
    "drizzle-orm": "^0.32.0",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "postcss": "^8.4.39",
    "pug": "^3.0.3"
  }
}
